{
  "manifest_version": 2,
  "name": "Pundit NYT Reader",
  "description": "News dashboard that delivers top New York Times stories to New Tab Page",
  "version": "0.1",
  "permissions": [
    "http://pundit-nyt-reader.herokuapp.com/",
    "http://pundit-nyt-reader.herokuapp.com/pundit",
    "http://www.w3.org/2000/svg", 
    "http://www.google.com/jsapi"
  ],
  "background": {
    "scripts": ["javascripts/pundit.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}